<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ian.mapper.CategoryMapper">


    <select id="selectByName" resultType="com.ian.entity.Category" parameterType="java.util.Set">
        select id, type, name, sort, create_time, update_time, create_user, update_user
        from category
        where id in
        <foreach collection="set" index="index" item="cateId" open="(" separator="," close=")">
            #{cateId}
        </foreach>
    </select>
</mapper>